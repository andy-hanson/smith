use Fun

‣‣ of message
	`
		throw new Error(message)

‣‣ try-catch try:Fun catch:Fun
	try-catch-finally try catch { }

‣‣ try-catch-finally try:Fun catch:Fun finally:Fun
	`
		var res;
		try {
			res = _try();
		}
		catch (error) {
			res = _catch(error);
		}
		finally {
			_finally();
		}
		return res;









