use ./Str

Str.do
	‣ ++ a:Str
		`this + a`

	‣‣ of a
		`
			switch (typeof a) {
				case 'undefined':
					return 'Undefined';
				case 'object':
					if (a == null)
						return 'Null';
					// fall through
				default:
					return a.toString();
			}

	#{
	+index-of-substring~ |needle
		. js-index-of
			`this.indexOf(needle)`
		(Some `this.indexOf(needle)`).filterOut -1.=_
	}#

	alias 'toUpperCase 'to-upper-case
	alias 'toLowerCase 'to-lower-case

	‣ split-whitespace
		`this.split("\\s+")`

	‣ size
		`this.length`

	# Iterator?
